                <div class="row">
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-1">
                            <div class="card-body">
                                <h3 class="card-title text-white">Registered Customers</h3>
                                <div class="d-inline-block">
                                <%int cnt=0; %>
                                <%for(Customer c:custList){ %>
                                	<%cnt++;%>
                                    <%} %>
                                    <h2 class="text-white"><%=cnt %></h2>
                                    <!-- <p class="text-white mb-0">Jan - March 2019</p> -->
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-users"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-2">
                            <div class="card-body">
                                <h3 class="card-title text-white">Net Banking Customers</h3>
                                <div class="d-inline-block">
                                <%int cntnetbanking=0;%>
                                <%for(NetBanking n:netbankingList){ %>
                                	<%cntnetbanking++; %>
                                <%} %>
                                    <h2 class="text-white"><%=cntnetbanking %></h2>
                                   
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-money"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
